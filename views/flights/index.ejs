
 <%- include("../partials/nav.ejs")%> 


 <main>
  <table class='table'>
    <thead>
      <tr>
        <th>Airline</th>
        <th>Airport</th>
        <th>Flight Number</th>
        <th>Departs</th>
        <th>Details</th>
      </tr>
    </thead>
    <tbody>
     
      <% flights.forEach(flight => { %>
        
        <tr>
          
          <td><%= flight.airline %></td>
          
          <td><%= flight.airport %></td>
          
          <td><%= flight.flightNo %></td>
          
          <td><%= flight.departs %></td>
          
          <td><a href="/flights/<%= flight._id %> ">Details</a></td>
        
        </tr>
     
        <% }) %>
    </tbody>
  </table>
</main>


<%- include("../partials/footer.ejs")%>